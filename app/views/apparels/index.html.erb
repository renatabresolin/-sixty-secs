
<h2>Todos LOOKS</h2>


<% @apparels.each do |apparel| %>
  <%= apparel.style %>
  <%= cl_image_tag apparel.photo.key, height: 300, width: 400, crop: :fill %>
  <ul>
    <% apparel.categories.each do |category| %>
      <li> <%= category.tag.category %> <%= category.tag.content %> </li>
    <% end %>
  </ul>
<% end %>

